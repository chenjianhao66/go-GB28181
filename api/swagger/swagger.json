                },
                "DeviceId": {
                    "type": "string"
                },
                "Manufacturer": {
                    "type": "string"
                },
                "Model": {
                    "type": "string"
                },
                "Name": {
                    "type": "string"
                },
                "Owner": {
                    "type": "string"
                },
                "ParentID": {
                    "type": "string"
                },
                "Parental": {
                    "type": "string"
                },
                "RegisterWay": {
                    "type": "string"
                },
                "SafetyWay": {
                    "type": "string"
                },
                "Secrecy": {
                    "type": "string"
                },
                "Status": {
                    "type": "string"
                },
                "createdAt": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "updatedAt": {
                    "type": "string"
                }
            }
        },
        "model.Device": {
            "type": "object",
            "properties": {
                "createdAt": {
                    "type": "string"
                },
                "deviceId": {
                    "type": "string"
                },
                "domain": {
                    "type": "string"
                },
                "expires": {
                    "type": "string"
                },
                "firmware": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "ip": {
                    "type": "string"
                },
                "keepalive": {
                    "type": "string"
                },
                "manufacturer": {
                    "type": "string"
                },
                "model": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "offline": {
                    "type": "integer"
                },
                "port": {
                    "type": "string"
                },
                "registerTime": {
                    "type": "string"
                },
                "transport": {
                    "type": "string"
                },
                "updatedAt": {
                    "type": "string"
                }
            }
        },
        "model.DeviceControl": {
            "type": "object",
            "properties": {
                "channelId": {
                    "description": "通道id",
                    "type": "string"
                },
                "command": {
                    "description": "控制的命令，取值为：left、right、down、up、downright、downleft、upright、upleft、zoomin、zoomout",
                    "type": "string"
                },
                "deviceId": {
                    "description": "设备id",
                    "type": "string"
                },
                "horizonSpeed": {
                    "description": "水平方向移动速度，取值：0-255",
                    "type": "integer"
                },
                "verticalSpeed": {
                    "description": "垂直方向移动速度，取值：0-255",
                    "type": "integer"
                },
                "zoomSpeed": {
                    "description": "变倍控制速度，取值：0-255",
                    "type": "integer"
                }
            }
        }
    },
    "securityDefinitions": {
        "BasicAuth": {
